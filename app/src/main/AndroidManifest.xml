<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.spider.myapplication">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.SEND_SMS"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:supportsRtl="true"
        android:label="@string/app_name"
        android:theme="@style/Base.Theme.AppCompat.Light.DarkActionBar">
        <activity android:name=".Activitys.Appsplash"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activitys.Mainactivity1"></activity>
        <activity android:name=".Activitys.Mainactivity2"></activity>
        <activity android:name=".Activitys.Mainactivity3"></activity>
        <activity android:name=".Activitys.Forsendsms"></activity>
        <activity android:name=".Activitys.Aboutapp"></activity>
        <activity android:name=".Activitys.Team"></activity>
        <activity android:name=".Activitys.Mainactivity"></activity>
        <activity android:name=".Activitys.Mainactivitymain"
            android:theme="@style/Theme.AppCompat.NoActionBar"></activity>

    </application>

</manifest>